@using TravelTripProje.Models.Sınıflar
@model List<Blog>
<div class="padding1">
    <h4>En Beğendiğim yerler</h4>
    <div class="box-left">

        @foreach (var item in Model)
        {

            <div class="bot-img">
                <img style="width:120px; height:65px" src="@item.BlogImage" alt="" />
            </div>
            <div class="bot-desc">
                <p>@item.Aciklama.Substring(0, 75) </p>
            </div>
            <div class="clear"></div>


        }

    </div>
    @Html.Action("Partial5", "Default")
</div>
