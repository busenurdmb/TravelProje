@using TravelTripProje.Models.Sınıflar
@model List<Blog>

    <div class="box1-left">

        @foreach (var item in Model)
        {

            <div class="bot-img">
                <img style="width:120px; height:65px" src="@item.BlogImage" alt="" />
            </div>
            <div class="bot-desc1">
                <p>@item.Aciklama.Substring(0, 75) </p>
            </div>
            <div class="clear"></div>


        }

    </div>

